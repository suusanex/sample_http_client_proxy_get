<Window x:Class="sample_http_client_proxy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sample_http_client_proxy"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Button DockPanel.Dock="Top" MinHeight="30" Click="OnMainBtn" Content="Main"></Button>
        <Button DockPanel.Dock="Top" Click="OnOldTypeBtn" MinHeight="30" Content="Old"></Button>
        <Button DockPanel.Dock="Top" MinHeight="30" Click="OnMainBtnDirectProxy" Content="MainDirectProxy"></Button>
        <Button DockPanel.Dock="Top" Click="OnOldTypeBtnDirectProxy" MinHeight="30" Content="OldDirectProxy"></Button>
        
    </DockPanel>
</Window>
